{
  "name": "@clovercoin/api",
  "version": "1.1.9",
  "main": "dist/app.js",
  "license": "MIT",
  "source": "src/app.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist/* && rm -rf ./build/*",
    "build": "tsc && ts-node build.mts",
    "start": "nodemon src/app.ts -e ts,mts,cts --watch src --ignore \"src/seeds/**/*.*\" --ignore \"src/migrations/**/*.*\"",
    "test-unit": "jest --config jest.unit.config.ts",
    "test-integration": "node --experimental-vm-modules $(yarn bin jest) --config jest.integration.config.ts",
    "typeorm": "typeorm-ts-node-esm",
    "migration:register": "ts-node bin/registerMigration.ts",
    "migration:create": "./bin/create-migration.sh",
    "migration:auto": "./bin/generate-automigration.sh",
    "migrate": "yarn typeorm migration:run -d src/db/migrationDataSource.ts",
    "seed": "ts-node src/seeds/gql/_seed.mts",
    "db:drop": "ts-node bin/drop-db.mts",
    "db:refresh": "ts-node bin/drop-db.mts && yarn migrate",
    "lint": "eslint ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.282.0",
    "@aws-sdk/client-secrets-manager": "^3.363.0",
    "@aws-sdk/node-http-handler": "^3.360.0",
    "@aws-sdk/s3-request-presigner": "^3.282.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@koa/cors": "^3.3.0",
    "@swc/helpers": "^0.5.1",
    "@types/koa__cors": "^3.1.1",
    "@vendia/serverless-express": "^4.10.4",
    "awilix": "^7.0.2",
    "bcrypt": "5.0.1",
    "class-validator": "0.14.0",
    "graphql": "^15.5.0",
    "graphql-request": "^6.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-graphql": "^0.12.0",
    "koa-mount": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "type-graphql-dataloader": "^0.5.0",
    "typeorm": "^0.3.17",
    "uuid": "^8.3.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@apollo/client": "3.7.1",
    "@aws-sdk/types": "^3.272.0",
    "@clovercoin/api-client": "workspace:packages/api-client",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@hyrious/esbuild-plugin-commonjs": "^0.2.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/koa-mount": "^4.0.1",
    "@types/node": "^20.4.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "axios": "1.2.0-alpha.1",
    "cross-fetch": "^3.1.5",
    "esbuild": "^0.18.11",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express-graphql": "^0.12.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.6.1",
    "nodemon": "^3.0.0",
    "parcel": "^2.9.3",
    "prettier": "^2.6.2",
    "testcontainers": "^9.0.0",
    "ts-jest": "^29.1.1",
    "ts-morph": "^19.0.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "type-fest": "^3.1.0",
    "typescript": "4.8.4",
    "unique-names-generator": "^4.7.1"
  },
  "graphql": {
    "schema": "./schema.gql",
    "documents": [
      "**/*.ts"
    ]
  },
  "engines": {
    "node": ">= 16"
  }
}
